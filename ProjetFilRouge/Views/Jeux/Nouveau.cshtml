@model Jeu

<h1>Nouveau Jeu</h1>
<div>
	<div class="form-content">
		<label asp-for=" @Model.Titre"></label>
		<input asp-for="@Model.Titre" />
		<span asp-validation-for="@Model.Titre" class="text-danger"></span>
	</div>
	<div class="form-content">
		<label asp-for="@Model.Description"></label>
		<input asp-for="@Model.Description" />
		<span asp-validation-for="@Model.Description" class="text-danger"></span>
	</div>
	<div class="form-content">
		<label asp-for="@Model.NombreJoueursRecommandes"></label>
		<input asp-for="@Model.NombreJoueursRecommandes" />
		<span asp-validation-for="@Model.NombreJoueursRecommandes" class="text-danger"></span>
	</div>
	<div class="form-content">
		<label asp-for="@Model.TempsJeuMoyen"></label>
		<input asp-for="@Model.TempsJeuMoyen" />
		<span asp-validation-for="@Model.TempsJeuMoyen" class="text-danger"></span>
	</div>
	<div class="form-content">
		<label asp-for="@Model.DateAjout"></label>
		<input asp-for="@Model.DateAjout" />
		<span asp-validation-for="@Model.DateAjout" class="text-danger"></span>
	</div>
	<input type="submit" value="Envoyer">
</div>

		@section Scripts {
    <partial name="_ValidationScriptsPartial" />
		}